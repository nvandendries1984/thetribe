{
  "name": "thetribe-discord-bot",
  "version": "1.0.0",
  "description": "Discord bot for The Tribe with slash commands and automod",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "deploy-commands": "node deploy-commands.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f discord-bot",
    "docker:restart": "docker-compose restart discord-bot"
  },
  "keywords": ["discord", "bot", "automod"],
  "author": "The Tribe",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.14.1",
    "mongoose": "^8.0.3",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}